@using DispatcherWeb.Web.Areas.App.Models.Common.Modals
@await Html.PartialAsync("~/Areas/app/Views/Common/Modals/_ModalHeader.cshtml", new ModalHeaderViewModel("Select quote"))

<div class="modal-body">
    <form role="form" novalidate class="form-validation">
        <div class="form-group">
            <label class="control-label">Quote</label>
            <select class="form-control quote-dropdown" id="PopupQuoteId"></select>
        </div>
    </form>
</div>

<div class="modal-footer">
    <button type="button" class="btn btn-primary save-button"><span>@L("OK")</span></button>
</div>