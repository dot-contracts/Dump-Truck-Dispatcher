@using DispatcherWeb.Web.Areas.App.Models.Common.Modals
@model ExtendedModalHeaderViewModel
<div class="modal-header">
    <div class="extended-title-container">
        <h5 class="modal-title text-ellipsis">
            @Model.Title
        </h5>
        <h5 class="modal-title text-ellipsis subtitle">
            @Model.ExtraText
        </h5>
    </div>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"></button>
</div>
