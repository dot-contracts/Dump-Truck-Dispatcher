  </ItemGroup>
  <ItemGroup>
    <None Remove="npm-debug.log" />
  </ItemGroup>
  <ItemGroup>
    <TypeScriptCompile Remove="wwwroot\Common\Scripts\dataList.ts" />
  </ItemGroup>
