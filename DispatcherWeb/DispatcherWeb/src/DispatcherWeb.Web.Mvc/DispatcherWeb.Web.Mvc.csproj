    <ProjectReference Include="..\..\..\..\ABP\src\Abp.Castle.Log4Net\Abp.Castle.Log4Net.csproj" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\DispatcherWeb.Web.Core\DispatcherWeb.Web.Core.csproj" />
